  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-init": "mkdirp build",
    "build:html": "cp app/*.html ./build",
    "build:js": "gulp build:js",
    "build": "npm-run-all -s build-init -p build:*",
    "watch-and-build:js": "gulp watch:js",
    "sync": "browser-sync start --reload-delay 500 --files 'build/**/*' --server build --port 5000",
    "api": "NODE_CONFIG_DIR='./server/config' nodemon server/ --watch ./server",
    "start": "npm-run-all -p sync api watch-and-build:js"
  },
